vector<vector<int> > $$varname$$ = vector<vector<int> >();
ifstream $$f$$($$filename$$);
int $$count$$ = 0;
vector<int> $$tmp_vector$$ = vector<int>();
while (!f.eof()) {
	int j;
	$$f$$ >> j;
	$$tmp_vector$$.push_back(j);
	$$count$$++;
	if ($$count$$ == $$numcolumns$$) {
		$$count$$ = 0;
		$$varname$$.push_back($$tmp_vector$$);
		$$tmp_vector$$ = vector<int>();
	}
}
$$f$$.close();

