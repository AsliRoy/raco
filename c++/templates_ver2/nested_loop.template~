
//loop over $$table$$
for (int $$index$$ = 0; $$index$$ < $$table$$.size(); ++$$index$$) {
    //if there is no match, continue
    if ($$hash$$.find($$table$$[$$index$$].a$$column$$) == $$hash$$.end()) {
        continue;
    }
    vector<vector<int> > $$new_table$$ = $$hash$$[$$table$$[$$index$$].a$$column$$];


